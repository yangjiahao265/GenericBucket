{
    "homepage": "https://github.com/yangjiahao265/GenericBucket",
    "description": "Adds some useful scoop alias",
    "version": "v0.0.4",
    "url": "https://raw.githubusercontent.com/snaphat/alias-additions/master/README.md",
    "hash": "1bb5878d806b2d273a84581668b38f436dcbbadfc4aadff2b0d3fe6dd1c81976",
    "installer": {
        "script": [
            "function global:Write-Host() {}",
            "scoop alias rm reinstall",
            "Remove-Item -Path Function:Write-Host",
            "Write-Host -NoNewline \"Adding alias for \"; Write-host -ForegroundColor green \"reinstall\"",
            "scoop alias add reinstall \"scoop uninstall $args; scoop install `\"$args`\"\" \"Update all apps and scoop\"",
        ]
    },
    "uninstaller": {
        "script": [
            "scoop alias rm reinstall",
        ]
    }
}
